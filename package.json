{
  "name": "steven-react-starter-kit",
  "version": "1.0.0",
  "description": "A reactjs starter kit",
  "keywords": [
    "Node",
    "React",
    "Redux",
    "Express"
  ],
  "license": "MIT",
  "repository": "https://github.com/chenfanggm/steven-react-redux-starter-kit",
  "homepage": "chenfang.me",
  "author": "Chen Fang",
  "main": "main.js",
  "engines": {
    "node": ">=8.1.0",
    "npm": "^5.0.0",
    "yarn": "^0.24.6"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "forever": "better-npm-run forever",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "lint": "eslint client tests server",
    "lint:fix": "npm run lint -- --fix"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec node bin/start",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/start",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "forever": {
      "command": "forever start -c node bin/start",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "better-npm-run": "0.1.0",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "http-status": "^1.0.1",
    "ip": "^1.1.5",
    "lodash-es": "^4.17.4",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-addons-linked-state-mixin": "^15.6.2",
    "react-cookie": "^2.1.2",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "showdown": "^1.8.6",
    "socket.io": "^2.0.4",
    "whatwg-fetch": "^2.0.3"
  },
